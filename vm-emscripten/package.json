{
  "name": "vm-emscripten",
  "version": "1.0.0",
  "description": "BlockProc vm",
  "main": "dist",
  "scripts": {
    "start": "http-server dist",
    "build": "powershell ./makeDebug.ps1",
    "deploy": "powershell ./makeDeploy.ps1 && copyfiles -f -V dist/vm.wasm dist/vm.js ../block-proc/src/assets/.",
    "cbuild": "nearleyc compiler/compiler.ne -o compiler/compiler.ts",
    "ctest": "ts-node-dev tests/compiler.ts",
    "rtest": "ts-node-dev tests/execute.ts"
  },
  "author": "Patryk Tomaszewski",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "http-server": "^14.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "nearley": "^2.20.1"
  }
}
